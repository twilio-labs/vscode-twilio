{
  "extends": ["eslint-config-twilio", "plugin:prettier/recommended"],
  "env": {
    "browser": false,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "camelcase": ["error", { "allow": ["^child_process"] }],
    "no-console": "off",
    "prefer-named-capture-group": "off"
  }
}
